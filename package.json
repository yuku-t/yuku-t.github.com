{
  "private": true,
  "repository": "yuku/yuku.github.io",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "build:entries": "ts-node --project ./script/tsconfig.json ./script/build-entries.ts",
    "build:next": "next build && next export && touch ./out/.nojekyll && echo yuku.takahashi.coffee > ./out/CNAME",
    "build": "run-s build:entries build:next",
    "dev": "next",
    "format:eslint": "yarn lint --fix",
    "format:prettier": "prettier --write '**/*.{scss,tsx,ts,jsx,js,mdx,json}'",
    "format": "run-s format:prettier format:eslint",
    "lint": "eslint '{pages,src,types}/**/*.{tsx,ts}'",
    "start": "run-s build:entries dev",
    "test": "run-s lint build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@mapbox/rehype-prism": "^0.4.0",
    "@mdx-js/loader": "^1.6.4",
    "@mdx-js/mdx": "^1.6.4",
    "@types/classnames": "^2.2.10",
    "@types/glob": "^7.1.1",
    "@types/next": "^9.0.0",
    "@types/prettier": "^2.0.1",
    "@types/react": "^16.9.35",
    "@types/rss": "^0.0.28",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "@zeit/next-sass": "^1.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "del": "^5.1.0",
    "glob": "^7.1.6",
    "htmltojsx": "^0.3.0",
    "moment": "^2.26.0",
    "next": "^9.4.2",
    "next-ga": "^2.3.4",
    "node-sass": "^4.14.1",
    "notebookjs": "^0.4.2",
    "null-loader": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rehype-katex": "^3.0.0",
    "rehype-parse": "^6.0.2",
    "rehype-stringify": "^7.0.0",
    "remark-footnotes": "^1.0.0",
    "remark-github": "^9.0.0",
    "remark-math": "^2.0.1",
    "remark-parse": "^8.0.2",
    "remark-rehype": "^7.0.0",
    "rss": "^1.2.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "unified": "^9.0.0",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  }
}
