{
  "private": true,
  "repository": "yuku/yuku.github.io",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "build:entries": "ts-node script/build-entries.ts",
    "build:next": "next build && next export && touch ./out/.nojekyll && echo yuku.takahashi.coffee > ./out/CNAME",
    "build": "run-s build:entries build:next",
    "deploy": "gh-pages -d out -b master -t",
    "dev": "next",
    "format": "prettier --write '**/*.{scss,tsx,ts,jsx,js,mdx,json}'",
    "start": "run-s build:entries watch",
    "test": "tslint '{pages,src,types}/**/*.{tsx,ts}'",
    "watch": "run-p watch:* dev",
    "watch:format": "onchange '**/*.{scss,tsx,ts,jsx,js,mdx,json}' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.3.1",
    "@mdx-js/loader": "^0.16.6",
    "@mdx-js/mdx": "^0.16.6",
    "@types/classnames": "^2.2.6",
    "@types/glob": "^7.1.1",
    "@types/next": "^7.0.5",
    "@types/react": "^16.7.18",
    "@zeit/next-mdx": "^1.2.0",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-loader": "^8.0.5",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.2.1",
    "chokidar": "^2.0.4",
    "classnames": "^2.2.6",
    "del": "^3.0.0",
    "glob": "^7.1.3",
    "htmltojsx": "^0.3.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.23.0",
    "next": "^7.0.2",
    "next-ga": "^2.3.4",
    "node-sass": "^4.12.0",
    "notebookjs": "^0.4.2",
    "null-loader": "^0.1.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rehype-katex": "^1.2.0",
    "rehype-parse": "^6.0.1",
    "rehype-stringify": "^6.0.0",
    "remark-github": "^7.0.6",
    "remark-math": "^1.0.5",
    "remark-parse": "^7.0.1",
    "remark-rehype": "^5.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "unified": "^8.3.2",
    "webpack": "^4.28.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "prettier": "^1.15.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all"
  },
  "babel": {
    "presets": [
      "next/babel",
      "@zeit/next-typescript/babel"
    ]
  }
}
